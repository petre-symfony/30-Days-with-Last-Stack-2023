<form method="post" action="{{ path('app_voyage_delete', {'id': voyage.id}) }}"
      onsubmit="return confirm('Are you sure you want to delete this item?');">
  <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ voyage.id) }}">
  <twig:Button
      class="text-white bg-red-600 hover:bg-red-700 focus:ring-4 focus:ring-red-300 focus:outline-none">
    Delete
  </twig:Button>
</form>
